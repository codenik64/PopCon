@model IEnumerable<PopsConfectionary14.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>All Registered Users</h2>
@*@Html.ActionLink("Archived Users", "DeleteUsers")*@

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
           
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
            @*@Html.ActionLink("Archive", "Remove", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
